rsed;\n    }\n    bidExt(b) {\n        this.state.resBid(b);\n    }\n\n    async mayBid(minPrice) {\n        const [account,] = this.context;\n        const balance = await Reach.balanceOf(account)\n\n        const fmt = (amt) => Reach.formatCurrency(amt, 4);\n        console.log(`Balance: ${fmt(balance)}, Minimum Bid: ${fmt(minPrice)}`);\n\n        return balance >= minPrice;\n    }\n\n    async storeSalt(salt) {\n        const saltFormatted = Reach.bigNumberToNumber(salt);\n        this.setState(prevState => ({\n            ...prevState,\n            salt: saltFormatted,\n        }));\n    }\n\n    async restoreSalt() {\n        return this.state.salt;\n    }\n\n    async restoreBid() {\n        return Reach.parseCurrency(this.state.amt);\n    }\n\n    async madeBid() {\n        this.setState({\n            appState: \"madeBid\",\n        });\n    }\n\n    async observeTimeout() {\n        this.setState({\n            appState: \"observeTimeout\",\n        });\n    }\n\n    async seeWinningBid(address, bid) {\n        const bidFormatted = Reach.formatCurrency(bid, 4);\n        this.setState({\n            appState: \"seeWinningBid\",\n            args: [address, bidFormatted],\n        });\n    }\n\n    render() {\n        return (<BuyerViews\n            appState={this.state.appState}\n            args={this.state.args}\n            bidReady={this.state.resBid !== null}\n            bid={this.bidExt} />);\n    }\n}","/Users/xianghw/Downloads/reach-react-template/src/build/index.main.mjs",[],"/Users/xianghw/Downloads/reach-react-template/src/components/participants/BuyerViews.js",[],"/Users/xianghw/Downloads/reach-react-template/src/components/participants/AuctionerViews.js",[],"/Users/xianghw/Downloads/reach-react-template/src/components/reach/Balance.js",[],"/Users/xianghw/Downloads/reach-react-template/src/components/reach/ConnectWallet.js",[],"/Users/xianghw/Downloads/reach-react-template/src/components/reach/FaucetButton.js",[],"/Users/xianghw/Downloads/reach